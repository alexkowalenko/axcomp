; ModuleID = 'factorial'
source_filename = "factorial"

declare void @WriteInt(i64)

declare void @WriteLn()

define internal i64 @factorial_factor(i64 %x1) {
entry:
  %x = alloca i64
  store i64 %x1, i64* %x
  %x2 = load i64, i64* %x
  call void @WriteInt(i64 %x2)
  call void @WriteLn()
  %x3 = load i64, i64* %x
  %0 = icmp sle i64 %x3, 1
  br i1 %0, label %then, label %ifcont

then:                                             ; preds = %entry
  ret i64 1

ifcont:                                           ; preds = %entry
  %x4 = load i64, i64* %x
  %x5 = load i64, i64* %x
  %subtmp = sub i64 %x5, 1
  %1 = call i64 @factorial_factor(i64 %subtmp)
  %multmp = mul i64 %x4, %1
  ret i64 %multmp
}

define i64 @output() {
entry:
  %0 = call i64 @factorial_factor(i64 10)
  ret i64 %0
}
